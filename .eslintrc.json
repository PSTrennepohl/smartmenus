{
  "root": true,
  "extends": [
    "plugin:unicorn/recommended",
    "xo",
    "xo/browser"
  ],
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "semi": [
      "error",
      "never"
    ],
    "unicorn/no-null": "off",
    "prefer-rest-params": "off",
    "capitalized-comments": "off",
    "comma-dangle": [
      "error",
      "never"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "max-depth": "off",
    "default-case": "off",
    "no-multi-assign": "off",
    "no-void": "off"
  }
}
